{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**", "cache/**"]
      },
      "dev": {
        "persistent": true,
        "cache": false
      },
      "start": {
        "persistent": true,
        "cache": false
      }
    }
  }
